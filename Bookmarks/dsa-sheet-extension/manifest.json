{
  "manifest_version": 3,
  "name": "DSA Sheet Navigator",
  "version": "1.0",
  "description": "Quick access to your complete DSA study materials with hierarchical navigation and search functionality.",
  "permissions": [],
  "action": {
    "default_popup": "popup.html",
    "default_title": "DSA Sheet Navigator",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["data/dsa_data.json"],
      "matches": ["<all_urls>"]
    }
  ]
}